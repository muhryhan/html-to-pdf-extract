{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
    "outDir": "./dist",
    "sourceMap": true,
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "lib": ["es6", "dom"],
    "target": "es6",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": ["./node_modules/@types", "src/@types"]
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}